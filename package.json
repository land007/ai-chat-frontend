  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "ts-node --project server-tsconfig.json server.ts",
    "server:build": "tsc --project server-tsconfig.json",
    "server:prod": "node dist/server.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "start:prod": "npm run build && npm run server:build && npm run server:prod",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"server.ts\"",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc --project server-tsconfig.json --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:3002",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
