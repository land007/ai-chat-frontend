{
  "name": "ai-chat-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "ai": "^5.0.72",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "pm2": "^6.0.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.1",
    "react-markdown": "^10.1.0",
    "react-scripts": "5.0.1",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "watch": "nodemon --signal SIGTERM --watch server.js --watch src --ext js,ts,tsx,jsx --ignore build --exec node server.js",
    "watch:server": "nodemon --signal SIGTERM --watch server.js --exec node server.js",
    "server:dev": "pm2 start ecosystem.config.js --env development --watch",
    "server:prod": "pm2 start ecosystem.config.js --env production",
    "server:stop": "pm2 stop ai-chat-server",
    "server:restart": "pm2 restart ai-chat-server",
    "server:delete": "pm2 delete ai-chat-server",
    "server:logs": "pm2 logs ai-chat-server",
    "server:status": "pm2 status",
    "dev": "concurrently \"npm run start\" \"npm run server:dev\"",
    "start:prod": "npm run build && npm run server:prod",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  }
}
